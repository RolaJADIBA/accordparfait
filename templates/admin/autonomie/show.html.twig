{% block title %}Autonomie{% endblock %}

{% block body %}
<div class="container">
    <div class="row mt-3">

    <h1>{{ autonomie.titre }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ autonomie.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ autonomie.titre }}</td>
